{
  "name": "nuxt-frigear-nu",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "lint": "eslint .",
    "lint:md":"npx mdclint 'content/**/*.md'",
    "clean": "rm -rf pnpm-lock.yaml node_modules .nuxt ",
    "cf:deploy": "npx wrangler --cwd .output/ deploy",
    "db:generate": "npx nuxt db generate",
    "db:migrate": "npx nuxt db migrate",
    "db:drop-all": "npx nuxt db drop-all",
    "db:reset": "pnpm run db:drop-all && pnpm run db:migrate"
  },
  "dependencies": {
    "@libsql/client": "^0.17.0",
    "@nuxthub/core": "https://pkg.pr.new/@nuxthub/core@759",
    "@nuxtjs/supabase": "2.0.3",
    "@unlok-co/nuxt-stripe": "6.0.2",
    "better-sqlite3": "^12.6.0",
    "drizzle-kit": "^0.31.8",
    "drizzle-orm": "^0.45.1",
    "minimatch": "^10.1.1",
    "nuxt": "^4.2.2",
    "simple-content-site": "^2.2.2",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@nuxt/eslint": "^1.12.1",
    "@nuxt/eslint-config": "^1.12.1",
    "eslint": "^9.39.2",
    "supabase": "^2.72.7",
    "wrangler": "^4.59.2"
  },
  "packageManager": "pnpm@10.19.0"
}